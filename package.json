{
  "name": "dynamic-text-html",
  "version": "1.0.0",
  "main": "deploy/barrel.js",
  "scripts": {
    "start": "parcel public/index.html --open --global app",
    "build": "parcel build public/index.html --public-url . --detailed-report --no-cache --out-dir ./build",
    "deploy": "parcel build src/barrel.js --public-url . --detailed-report --no-cache --out-dir ./deploy --target node",
    "watch": "parcel watch public/index.html --public-url . --out-dir watch/ --no-cache"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.3.0",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "marked": "^0.4.0"
  }
}
