    "@types/node": "^10.12.7",
    "@types/webpack": "^4.4.19",
    "@types/webpack-env": "^1.13.6",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "parcel-bundler": "^1.10.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "marked": "^0.5.1"
